{
  "expo": {
    "name": "Kotakbon Admin",
    "slug": "KotakbonAdmin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconKBA-new.png",
    "splash": {
      "image": "./assets/splashKBA-new.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.admin.kotakbonadmin",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-new.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
